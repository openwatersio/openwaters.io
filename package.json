{
  "name": "openwaters-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "website",
    "api"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w website\" \"npm run dev -w api\"",
    "dev:website": "npm run dev -w website",
    "dev:api": "npm run dev -w api",
    "build": "npm run build -w website"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@neaps/api": "^0.3.3",
    "@neaps/tide-database": "^0.6.20260218",
    "@neaps/tide-predictor": "^0.7.0",
    "neaps": "^0.5.1"
  }
}
